{
	"name": "@xats/eslint-config",
	"type": "module",
	"version": "1.2.10",
	"description": "",
	"author": "Xat <i@xat.sh> (https://xat.sh)",
	"license": "MIT",
	"keywords": [
		"eslint",
		"eslint-config"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "nlx @eslint/config-inspector --config eslint.config.js",
		"prepare": "lefthook install && nr build",
		"release": "bumpp && npm publish",
		"build": "nr type:gen && tsup --format esm,cjs --clean --dts",
		"type:gen": "tsx scripts/typegen.ts",
		"type:check": "tsc --noEmit",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"peerDependencies": {
		"eslint": "^9.5.0"
	},
	"dependencies": {
		"@eslint-react/eslint-plugin": "^1.17.3",
		"@html-eslint/eslint-plugin": "^0.30.0",
		"@html-eslint/parser": "^0.30.0",
		"@stylistic/eslint-plugin": "^2.11.0",
		"@typescript-eslint/eslint-plugin": "^8.17.0",
		"@typescript-eslint/parser": "^8.17.0",
		"@unocss/eslint-plugin": "0.65.0",
		"astro-eslint-parser": "^1.1.0",
		"eslint-config-flat-gitignore": "^0.3.0",
		"eslint-flat-config-utils": "^0.4.0",
		"eslint-merge-processors": "^0.1.0",
		"eslint-plugin-astro": "^1.3.1",
		"eslint-plugin-html": "^8.1.2",
		"eslint-plugin-import-x": "^4.5.0",
		"eslint-plugin-jsdoc": "^50.6.0",
		"eslint-plugin-jsonc": "^2.18.2",
		"eslint-plugin-n": "^17.14.0",
		"eslint-plugin-perfectionist": "^4.2.0",
		"eslint-plugin-react-hooks": "5.0.0",
		"eslint-plugin-react-refresh": "^0.4.16",
		"eslint-plugin-regexp": "^2.7.0",
		"eslint-plugin-tailwindcss": "^3.17.5",
		"eslint-plugin-toml": "^0.12.0",
		"eslint-plugin-unicorn": "^56.0.1",
		"eslint-plugin-unused-imports": "^4.1.4",
		"eslint-plugin-yml": "^1.16.0",
		"eslint-typegen": "^0.3.2",
		"globals": "^15.13.0",
		"jsonc-eslint-parser": "^2.4.0",
		"local-pkg": "^0.5.1",
		"toml-eslint-parser": "^0.10.0",
		"yaml-eslint-parser": "^1.2.3"
	},
	"devDependencies": {
		"@antfu/ni": "^0.23.1",
		"@types/node": "^22.10.1",
		"astro": "^5.0.2",
		"bumpp": "^9.8.1",
		"eslint": "^9.16.0",
		"lefthook": "^1.8.5",
		"react": "^18.3.1",
		"tailwindcss": "^3.4.16",
		"tsup": "^8.3.5",
		"tsx": "^4.19.2",
		"typescript": "^5.7.2",
		"unocss": "0.65.0"
	}
}
